        {
            name : "B.Tech, Computers And Communication Engg",
            location : "Manipal Institute Of Technology, Manipal",
            desc : "8.52 / 10 CGPA lorem ipsum ipsum",
        },
        {
            name : "CBSE, 12th Grade",
