mixin education(name, location, desc)
    .box.box--edu
        h3.text--box.text--main #{name}
        h6.text--box.text--sub
            i #{location}
        .text--box
            p #{desc}

h2.text--title.text--smaller Education
for val in edudata
    +education(val.name, val.location, val.desc)